html
	head
		title Review Form
		
	body
		h1= stucourse
		h2 Professor #{stufaculty}
		h3= stuname
		h3= usn

		- var url = "http://192.168.43.215:3000/login/student/TakeReview/SubmitReview"
		
		form(action="#{url}", method="post", enctype="application/json")
			- for(var i = 0; i < quest.length; i++) {

			fieldset
				legend(id="msg")= quest[i].qmsg
				input(type="radio", name= "feed"+i, value="1", id="1")
				label(for="1") Yes
				input(type="radio", name= "feed"+i, value="2", id="2")
				label(for="2") No
				input(type="radio", name= "feed"+i, value="3", id="3")
				label(for="3") Average
				input(type="radio", name= "feed"+i, id="4", value="4", checked="true")
				label(for="4") Not Applicable
			- }
			input(type="hidden", name="cname", value= stucourse)
			input(type="hidden", name="usn", value= usn)
			input(type="hidden", name="numQuest", value= quest.length)
			input(type="submit", name="Submit", value="Submit")
			
				


			
					



			




	